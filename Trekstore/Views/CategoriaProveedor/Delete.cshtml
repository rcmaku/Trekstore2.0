@model Trekstore.Models.CategoriaProveedor

@{
    ViewData["Title"] = "Delete";
}
<video autoplay muted loop class="background-video">
    <source src="~/videos/video5.mp4" type="video/mp4">
    Your browser does not support the video tag.
</video>
<h1>Eliminar</h1>

<h3>Seguro que desea eliminar esto?</h3>
<div>
    <h4>Tipo de Proveedor</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CategoriaProveedorNombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoriaProveedorNombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CategoriaProveedorDescripcion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoriaProveedorDescripcion)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CategoriaProveedorID" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar</a>
    </form>
</div>
