@model IEnumerable<Trekstore.Models.TipoDePago>

@{
    ViewData["Title"] = "Index";
}
<video autoplay muted loop class="background-video">
    <source src="~/videos/video5.mp4" type="video/mp4">
    Your browser does not support the video tag.
</video>
<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.tipoPago)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.tipoPago)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.tipoPagoID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.tipoPagoID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.tipoPagoID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
